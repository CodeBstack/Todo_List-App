
* {
    margin: 0;
    padding: 0;
    box-sizing: inherit;
  }
  
  html {
    /* font-size: 62.5%; */
    box-sizing: border-box;
  }
  
  body {
    font-family: 'Josefin Sans', sans-serif;
    font-weight: 300;
    color: #444;
    line-height: 1.9;
    background-color: #ffffff;
  }

  .container{
    max-width: 100%;
  }
  .header{
    background-image: linear-gradient hsl(192, 100%, 67%) to hsl(280, 87%, 65%));
  }