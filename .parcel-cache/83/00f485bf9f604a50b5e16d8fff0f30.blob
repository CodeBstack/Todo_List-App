        <div class="list-footer">
          <p class="counts">0 items left</p>
          <div class="footer-nav">
            <button class="footer-nav-1 all">All</button>
            <button class="footer-nav-1 active">Active</button>
            <button class="footer-nav-1 completed">Completed</button>
          </div>
          <button class="footer-nav-1 clear">Clear Completed</button>
        </div>
      </div>
