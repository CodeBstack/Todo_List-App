* {
  margin: 0;
  padding: 0;
  box-sizing: inherit;
}

html {
  /* font-size: 62.5%; */
  box-sizing: border-box;
}

body {
  font-family: "Josefin Sans", sans-serif;
  font-weight: 300;
  color: #444;
  line-height: 1.9;
  background-color: #ffffff;
}

.container {
  max-width: 100%;
  background-image: url(9d15dbe1bd9daebadfeb9fc230e9618b);
  background-position: center;
  background-size: cover;
}
.header{
    background-image: linear-gradient hsl(192, 100%, 67%) to hsl(280, 87%, 65%), url(9d15dbe1bd9daebadfeb9fc230e9618b);
  
}
